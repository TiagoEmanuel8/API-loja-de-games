{
  "name": "api-loja-de-games",
  "version": "1.0.0",
  "description": "# Boas vindas ao projeto API-loja-de-games",
  "main": "./build/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && ts-node ./src/server.ts",
    "nodemon": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/server.ts\"",
    "start": "node .",
    "sequelize": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 10000 --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.7",
    "chai-http": "4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "jest": "^29.5.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "mysql2": "^3.2.4",
    "sequelize": "^6.31.0",
    "shelljs": "^0.8.5",
    "sinon": "^15.0.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/shelljs": "^0.8.12",
    "@types/sinon": "^10.0.14",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
